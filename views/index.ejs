<h2>Recently Added</h2>
<% books.forEach(book => { %>
    <a href="/books/<%= book.id %>">
        <div className="book">
            <div>
                <img height="150px" width="100px" src="<%= book.coverPath %>" alt="book cover">
            </div>
            <div><%= book.title %></div>
        </div>
    </a>
<% }) %>